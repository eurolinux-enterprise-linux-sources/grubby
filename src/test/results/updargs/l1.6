prompt
timeout=50
default=linux
boot=/dev/md1
map=/boot/map
install=/boot/boot.b
message=/boot/message
linear

disk=/dev/sdm
	bios=0x80
disk=/dev/sdn
	bios=0x81

image=/boot/vmlinuz-2.4.18-4
	label=linux-up
	read-only
	initrd=/boot/initrd-2.4.18-4.img
	append="foo root=LABEL=/ bar"

image=/boot/vmlinuz-2.4.18-4smp
	label=linux
	read-only
	initrd=/boot/initrd-2.4.18-4smp.img
	append="foo root=LABEL=/ bar"
