prompt
timeout=50
boot=/dev/hda
map=/boot/map
install=/boot/boot.b
message=/boot/message
linear

default=linux
image=/boot/new-kernel.img
        label=title
        read-only
        initrd=/boot/new-initrd
        append="root=LABEL=/"

image=/boot/vmlinuz-2.4.18-4
        label=linux
        read-only
        initrd=/boot/initrd-2.4.18-4.img
        append="root=LABEL=/"

other=/dev/hda2
        label=DOS
        optional

